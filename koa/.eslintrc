{
	"extends": "eslint:recommended",
	"env": {
		"browser": true,
		"node": true,
		"es6": true,
		"commonjs": true
	},
	"rules": {
		"semi": ["error", "always"],
		"quotes": "off",
		"no-console": "off",
		"no-unreachable": "off",
		"no-redeclare": "warn",
		"no-cond-assign": 2,
		"no-dupe-args": 2,
		"no-empty": 1,
		"no-inner-declarations": 1,
		"no-caller": 2,
		"no-eval": 2,
		"no-var": 2
	}
}
